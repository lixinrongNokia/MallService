<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping
        package="cn.iliker.mall.entity">

    <class name="Integralspending" table="integralspending">
        <id name="integralspendingID">
            <generator class="native"/>
        </id>
        <property name="spendAmount" type="java.lang.Integer" not-null="true"/>
        <property name="spendDESC" type="java.lang.String" insert="false"/>
        <property name="spendTime" type="java.util.Date" insert="false"/>
        <property name="phone" type="java.lang.String" not-null="true"/>
    </class>

</hibernate-mapping>