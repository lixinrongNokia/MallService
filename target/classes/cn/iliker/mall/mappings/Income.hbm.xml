<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="cn.iliker.mall.entity.Income" table="income">
        <id name="incomeID" type="java.lang.Integer">
            <generator class="native"/>
        </id>
        <property name="incomeTime" type="java.util.Date" insert="false"/>
        <property name="incomeDESC" type="java.lang.String" length="50" not-null="true"/>
        <property name="incomeAmount" type="java.lang.Double" not-null="true"/>
        <property name="phone" type="java.lang.String" length="32" not-null="true"/>
    </class>
</hibernate-mapping>
